pipeline {
    agent {
       label "deploy"
    }
    stages {
          stage("connect") {
             steps {
               script {
                   sh '''
                   make image NAME=tracker FILE=Modules/tracker/Dockerfile VERSION=0.0.2
                   make push NAME=tracker VERSION=0.0.2
                   make pull NAME=tracker VERSION=0.0.2
                   '''
               }
             }
          }
        }
}